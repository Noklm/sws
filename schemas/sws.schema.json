{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id":"./sws.schema.json",
  "title":"sws scripts",
  "type": "object",
  "properties": {
      "scripts":      { 
          "type": "object",
          "description": "You can add command lines scripts"
      }
  },
